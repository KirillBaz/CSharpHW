// Задача 66: Задайте значения M и N. Напишите программу, которая найдёт сумму натуральных элементов в промежутке от M до N.

// M = 1; N = 15 -> 120
// M = 4; N = 8. -> 30

int SumIntEl(int m, int n)
{
    if(n>=m) return n+SumIntEl(m, n-1);
    else return 0;
}
int x= SumIntEl(1, 15);
Console.WriteLine(x);
